{
    "prototypes": [{
            "extensions": "*.txt",
            "expressions": [{
                    "pattern": ".*escuela.*",
                    "flags": 2,
                    "condition": "-"
                }],
            "id": "excluder"
        }, {
            "extensions": "*.jsp",
            "expressions": [{
                    "pattern": ".*casa.*",
                    "flags": 2,
                    "condition": "+"
                }],
            "id": "base"
        }],
    "cleaners": [{
            "id": "c1",
            "description": "",
            "rules": [{
                    "pattern": ".*casa.*",
                    "replace": "casucha",
                    "flags": 2,
                    "ruleClass": "core.rules.ReplaceText"
                }]
        }, {
            "id": "c2",
            "description": "",
            "rules": [{
                    "pattern": ".*(aqui va una).*",
                    "replace": "$1 escuela",
                    "flags": 2,
                    "ruleClass": "core.rules.ReplaceText"
                }],
            "prototype": "excluder"
        },
        {
            "id": "c3",
            "description": "",
            "rules": [{
                    "pattern": ".*(aqui va una).*",
                    "replace": "$1 escuela",
                    "flags": 2,
                    "ruleClass": "core.rules.ReplaceText"
                }],
            "prototype": "excluder"
        }],
    "lastWorkingDirectory": null,
    "name": null,
    "description": null
}


