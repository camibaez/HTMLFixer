{
    "prototypes": [{
        "id": "base",
        "extensions": "*.jsp",
        "expressions": [{"pattern" : ".*oncontextmenu=\"showPopUp\\(.*", "flags" : 2, "condition" : "+"}]
    },
    {
        "id": "excluder",
        "extensions": "*.jsp",
        "expressions": [{"pattern" : ".*<script.*src=\".*javascripts/optMenu\\.jsp\".*>.*</script>.*", "flags" : 2, "condition" : "-"}]
    }
    ],
    "cleaners": [{
            "id": "c1",
            "description": "",
            "rules": [{
                    "pattern": "(<script.*/pages/e/javascripts/eIBS\\.jsp\".*>.*</script>)",
                    "replace": "$1 \n <script src=\"<%=request.getContextPath()%>/pages/e/javascripts/optMenu.jsp\"></script>",
                    "flags": 2,
                    "ruleClass": "core.rules.ReplaceText"
                }],
            "prototype" : "exluder"
        },
        {
            "id": "c2",
            "description": "",
            "rules": [{
                    "pattern": "(.*oncontextmenu=\"showPopUp\\(.*)",
                    "replace": "class=\"context-menu-help\" onmousedown=\"showPopUp(",
                    "flags": 2,
                    "ruleClass": "core.rules.ReplaceText"
                }]
        }],
    "lastWorkingDirectory": "conf\\test-files\\BigChangeTest\\files\\",
    "name": null,
    "description": null
}